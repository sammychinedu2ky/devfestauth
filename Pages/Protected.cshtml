@page
@model DevFestAuth.Pages.ProtectedModel
@{
  ViewData["Title"] = "Protected";
}

<div class="row" style="margin-top: 50px">
  <div class="valign-wrapper" style="justify-content: center">
    <h1 class="blue-text lighten-3">
      @{
        var claimsDiy = User.Claims.ToDictionary(x => x.Type);
        <span>Hello, @claimsDiy["FullName"].Value (Protected)</span><span class="expand">😎</span>
      }
    </h1>
  </div>
</div>